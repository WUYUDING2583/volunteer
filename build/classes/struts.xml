<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="strutsBean" extends="struts-default"
		namespace="/">
		<action name="login" class="volunteer.action.AdminAction"
			method="login">
			<result name="success" type="redirect">/Admin/main.jsp</result>
			<result name="fail">/Admin/index.jsp</result>
			<result name="input">/Admin/index.jsp</result>
		</action>
		<action name="alterPsw" class="volunteer.action.AdminAction"
			method="alterPsw">
			<result name="success" type="stream">
				<param name="contentType">text/html; charset=utf-8</param>
				<param name="inputName">inputStream</param>
			</result>
		</action>
		<action name="exit" class="volunteer.action.AdminAction"
			method="exit">
			<result name="success" type="redirect">/Admin/index.jsp</result>
		</action>
		<action name="isRegister" class="volunteer.action.FrontAction" method="isRegister"/>
		<action name="insertUserMess" class="volunteer.action.FrontAction" method="insertUserMess"/>
		<action name="viewActivity" class="volunteer.action.FrontAction" method="viewActivity"/>
		<action name="viewApply" class="volunteer.action.FrontAction" method="viewApply"/>
		<action name="apply" class="volunteer.action.FrontAction" method="apply"/>
		<action name="searchManHour" class="volunteer.action.FrontAction" method="searchManHour"/>
		<action name="isRegister" class="volunteer.action.FrontAction" method="isRegister"/>
		<action name="cancelApply" class="volunteer.action.FrontAction" method="cancelApply"/>
		<action name="updateUserMess" class="volunteer.action.FrontAction" method="updateUserMess"/>
		<action name="userMess" class="volunteer.action.FrontAction" method="userMess"/>
		
	</package>
</struts>