<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD
3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="volunteer.po.ActBase" table="activityinfo" catalog="volunteer" polymorphism="explicit">
		<id name="Ano" type="string">
			<column name="Ano" />
		</id>
		<property name="Adate" type="string">
			<column name="Adate" />
		</property>
		<property name="Aname" type="string">
			<column name="Aname" />
		</property>
		<property name="Address" type="string">
			<column name="Address" />
		</property>
		<property name="Arequest" type="string">
			<column name="Arequest" />
		</property>
		<property name="publishTime" type="string">
			<column name="publishTime" />
		</property>
		<property name="picture" type="string">
			<column name="picture" />
		</property>
		<property name="Adeadline" type="string">
			<column name="Adeadline" />
		</property>
	</class>
</hibernate-mapping>
