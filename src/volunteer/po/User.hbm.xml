<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD
3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="volunteer.po.User" table="account" catalog="volunteer">
		<id name="No" type="string">
			<column name="No" />
		</id>
		<property name="Name" type="string">
			<column name="Name" />
		</property>
		<property name="Sex" type="string">
			<column name="Sex" />
		</property>
		<property name="Cclass" type="string">
			<column name="Class" />
		</property>
		<property name="Vno" type="string">
			<column name="Vno" />
		</property>
		<property name="Phone" type="string">
			<column name="Phone" />
		</property>
		<property name="leader" type="integer">
			<column name="isLeader" />
		</property>
		<property name="college" type="string">
			<column name="College" />
		</property>
		<property name="openid" type="string">
			<column name="openid" />
		</property>
		<set name="manhours" inverse="false" cascade="all" lazy="true">
			<key column="No" />
			<one-to-many class="cn.edu.zjut.po.ManHour" />
		</set>
		<set name="applies" inverse="false" cascade="all" lazy="true">
			<key column="No" />
			<one-to-many class="cn.edu.zjut.po.Apply" />
		</set>

	</class>
</hibernate-mapping>
